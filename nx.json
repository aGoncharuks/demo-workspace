{
  "npmScope": "demo-workspace",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin-change-audit": {
      "tags": ["scope:admin"]
    },
    "admin-client": {
      "tags": ["scope:admin"]
    },
    "admin-client-e2e": {
      "tags": [],
      "implicitDependencies": ["admin-client"]
    },
    "admin-company-management": {
      "tags": ["scope:admin"]
    },
    "app-one-api": {
      "tags": ["scope:customer"]
    },
    "app-one-client": {
      "tags": ["scope:customer"]
    },
    "app-one-client-e2e": {
      "tags": [],
      "implicitDependencies": ["app-one-client", "app-one-api"]
    },
    "app-one-shared": {
      "tags": ["scope:customer"]
    },
    "app-three-client": {
      "tags": ["scope:customer"]
    },
    "app-three-client-e2e": {
      "tags": [],
      "implicitDependencies": ["app-three-client"]
    },
    "app-two-client": {
      "tags": ["scope:customer"]
    },
    "app-two-client-e2e": {
      "tags": [],
      "implicitDependencies": ["app-two-client"]
    },
    "core-auth": {
      "tags": ["scope:client"]
    },
    "core-message-bus": {
      "tags": ["scope:customer"]
    },
    "core-permissions": {
      "tags": ["scope:customer"]
    },
    "shell-client": {
      "tags": ["scope:customer"]
    },
    "shell-client-e2e": {
      "tags": [],
      "implicitDependencies": ["shell-client"]
    }
  }
}
