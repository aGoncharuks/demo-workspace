{
  "version": 2,
  "projects": {
    "app-one-api": {
      "root": "apps/app-one/app-one-api",
      "sourceRoot": "apps/app-one/app-one-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-one/app-one-api",
            "main": "apps/app-one/app-one-api/src/main.ts",
            "tsConfig": "apps/app-one/app-one-api/tsconfig.app.json",
            "assets": ["apps/app-one/app-one-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/app-one/app-one-api/src/environments/environment.ts",
                  "with": "apps/app-one/app-one-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "app-one-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-one/app-one-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-one/app-one-api"],
          "options": {
            "jestConfig": "apps/app-one/app-one-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-one-client": {
      "projectType": "application",
      "root": "apps/app-one/app-one-client",
      "sourceRoot": "apps/app-one/app-one-client/src",
      "prefix": "demo-workspace",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-one/app-one-client",
            "index": "apps/app-one/app-one-client/src/index.html",
            "main": "apps/app-one/app-one-client/src/main.ts",
            "polyfills": "apps/app-one/app-one-client/src/polyfills.ts",
            "tsConfig": "apps/app-one/app-one-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/app-one/app-one-client/src/favicon.ico",
              "apps/app-one/app-one-client/src/assets"
            ],
            "styles": ["apps/app-one/app-one-client/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/app-one/app-one-client/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/app-one/app-one-client/src/environments/environment.ts",
                  "with": "apps/app-one/app-one-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/app-one/app-one-client/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "app-one-client:build:production"
            },
            "development": {
              "browserTarget": "app-one-client:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 5100,
            "proxyConfig": "apps/app-one/app-one-client/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-one-client:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app-one/app-one-client/src/**/*.ts",
              "apps/app-one/app-one-client/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-one/app-one-client"],
          "options": {
            "jestConfig": "apps/app-one/app-one-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-one-client-e2e": {
      "root": "apps/app-one/app-one-client-e2e",
      "sourceRoot": "apps/app-one/app-one-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-one/app-one-client-e2e/cypress.json",
            "devServerTarget": "app-one-client:serve:development",
            "tsConfig": "apps/app-one/app-one-client-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-one-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-one/app-one-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "app-one-shared": {
      "root": "libs/app-one/shared",
      "sourceRoot": "libs/app-one/shared/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/app-one/shared/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/app-one/shared"],
          "options": {
            "jestConfig": "libs/app-one/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-two-client": {
      "root": "apps/app-two/app-two-client",
      "sourceRoot": "apps/app-two/app-two-client/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-two/app-two-client",
            "index": "apps/app-two/app-two-client/src/index.html",
            "main": "apps/app-two/app-two-client/src/main.tsx",
            "polyfills": "apps/app-two/app-two-client/src/polyfills.ts",
            "tsConfig": "apps/app-two/app-two-client/tsconfig.app.json",
            "assets": [
              "apps/app-two/app-two-client/src/favicon.ico",
              "apps/app-two/app-two-client/src/assets"
            ],
            "styles": ["apps/app-two/app-two-client/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "apps/app-two/app-two-client/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app-two/app-two-client/src/environments/environment.ts",
                  "with": "apps/app-two/app-two-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "app-two-client:build",
            "hmr": true,
            "port": 5200
          },
          "configurations": {
            "production": {
              "buildTarget": "app-two-client:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app-two/app-two-client/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-two/app-two-client"],
          "options": {
            "jestConfig": "apps/app-two/app-two-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-two-client-e2e": {
      "root": "apps/app-two/app-two-client-e2e",
      "sourceRoot": "apps/app-two/app-two-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-two/app-two-client-e2e/cypress.json",
            "devServerTarget": "app-two-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-two-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-two/app-two-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "core-client-message-bus": {
      "root": "libs/core/client-message-bus",
      "sourceRoot": "libs/core/client-message-bus/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/client-message-bus/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core/client-message-bus"],
          "options": {
            "jestConfig": "libs/core/client-message-bus/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shell-client": {
      "projectType": "application",
      "root": "apps/shell/shell-client",
      "sourceRoot": "apps/shell/shell-client/src",
      "prefix": "demo-workspace",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/shell/shell-client",
            "index": "apps/shell/shell-client/src/index.html",
            "main": "apps/shell/shell-client/src/main.ts",
            "polyfills": "apps/shell/shell-client/src/polyfills.ts",
            "tsConfig": "apps/shell/shell-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/shell/shell-client/src/favicon.ico",
              "apps/shell/shell-client/src/assets"
            ],
            "styles": ["apps/shell/shell-client/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/shell/shell-client/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/shell/shell-client/src/environments/environment.ts",
                  "with": "apps/shell/shell-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/shell/shell-client/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "shell-client:build:production"
            },
            "development": {
              "browserTarget": "shell-client:build:development",
              "port": 5000
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell-client:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/shell/shell-client/src/**/*.ts",
              "apps/shell/shell-client/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/shell/shell-client"],
          "options": {
            "jestConfig": "apps/shell/shell-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shell-client-e2e": {
      "root": "apps/shell/shell-client-e2e",
      "sourceRoot": "apps/shell/shell-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/shell/shell-client-e2e/cypress.json",
            "devServerTarget": "shell-client:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shell-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shell/shell-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "app-one-client"
}
