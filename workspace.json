{
  "version": 2,
  "projects": {
    "core-client-message-bus": {
      "root": "libs/core/client-message-bus",
      "sourceRoot": "libs/core/client-message-bus/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/client-message-bus/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core/client-message-bus"],
          "options": {
            "jestConfig": "libs/core/client-message-bus/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-api": {
      "root": "apps/demo/demo-api",
      "sourceRoot": "apps/demo/demo-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo/demo-api",
            "main": "apps/demo/demo-api/src/main.ts",
            "tsConfig": "apps/demo/demo-api/tsconfig.app.json",
            "assets": ["apps/demo/demo-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/demo/demo-api/src/environments/environment.ts",
                  "with": "apps/demo/demo-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "demo-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo/demo-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo/demo-api"],
          "options": {
            "jestConfig": "apps/demo/demo-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-client": {
      "projectType": "application",
      "root": "apps/demo/demo-client",
      "sourceRoot": "apps/demo/demo-client/src",
      "prefix": "demo-workspace",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo/demo-client",
            "index": "apps/demo/demo-client/src/index.html",
            "main": "apps/demo/demo-client/src/main.ts",
            "polyfills": "apps/demo/demo-client/src/polyfills.ts",
            "tsConfig": "apps/demo/demo-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/demo/demo-client/src/favicon.ico",
              "apps/demo/demo-client/src/assets"
            ],
            "styles": ["apps/demo/demo-client/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/demo/demo-client/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo/demo-client/src/environments/environment.ts",
                  "with": "apps/demo/demo-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/demo/demo-client/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-client:build:production"
            },
            "development": {
              "browserTarget": "demo-client:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 5100,
            "proxyConfig": "apps/demo/demo-client/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-client:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo/demo-client/src/**/*.ts",
              "apps/demo/demo-client/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo/demo-client"],
          "options": {
            "jestConfig": "apps/demo/demo-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-client-e2e": {
      "root": "apps/demo/demo-client-e2e",
      "sourceRoot": "apps/demo/demo-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo/demo-client-e2e/cypress.json",
            "devServerTarget": "demo-client:serve:development",
            "tsConfig": "apps/demo/demo-client-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo/demo-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "demo-shared": {
      "root": "libs/demo/shared",
      "sourceRoot": "libs/demo/shared/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/demo/shared/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/demo/shared"],
          "options": {
            "jestConfig": "libs/demo/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "demo-client"
}
